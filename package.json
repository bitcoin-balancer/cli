  "private": false,
  "version": "1.0.6",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-lib-builder --tsconfig=tsconfig.build.json",
    "test": "echo \"Error: tests are executed with  npm run test:(integration|unit|bench)\" && exit 1",
